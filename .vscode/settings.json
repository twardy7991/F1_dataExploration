{
    /* otherwise we get import errors on base hook in local IDE, 
    works in arflow tho but its cleaner */
    "python.analysis.extraPaths": [
    "${workspaceFolder}/airflow_project/plugins",
    "${workspaceFolder}/airflow_project/dags"
    ]
    ,
    "python.envFile": "${workspaceFolder}/.env",
    "python.testing.pytestArgs": [
        "airflow_project"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}